<%= render '/partial/navlist' %>
<h1>コンディショニングレポート全記録</h1>
<%= link_to 'Back', admin_path, class: 'btn btn-default' %>
<table class="table table-hover table-responsive table-striped">
  <thead class="thead-default">
  <tr>
    <th>User</th>
    <th>登録日時</th>
    <th>体重</th>
    <th>疲労度</th>
    <th>睡眠時間</th>
    <th>吐き気</th>
    <th>備考</th>
  </tr>
  </thead>
  <tbody>
  <% @reports.reverse_order.each do |report| %>
      <tr>
        <td><%= report.user_id %></td>
        <td><%= report.created_at %></td>
        <td><%= report.weight %>kg</td>
        <td><%= report.tired %></td>
        <td><%= report.sleep %></td>
        <td><%= report.nausea %></td>
        <td><%= report.other %></td>
        <!--<td><%= link_to 'Edit', edit_report_path(report) %></td>-->
        <!--<td><%= link_to 'Destroy', report, method: :delete, data: {confirm: 'Are you sure?'} %></td>-->
      </tr>
  <% end %>
  </tbody>
</table>
<br>
